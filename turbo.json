{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "ui#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^web#build", "^docs#build", "^electronrenderer#build"]
    },
    "docs#build": {
      "outputs": ["build/**"]
    },
    "web#build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "electronrenderer#build": {
      "outputs": ["build/**"],
      "dependsOn": ["^electronbuilder#build"]
    },
    "electronmain#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^electronbuilder#build"]
    },
    "electronbuilder#build": {
      "outputs": ["dist/**"]
    },
    "build": {},
    "clean": {
      "cache": false
    }
  }
}
