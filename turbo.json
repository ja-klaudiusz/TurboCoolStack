{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "ui#build": {
      "outputs": ["dist/**"]
    },
    "docs#build": {
      "dependsOn": ["^ui#build"],
      "outputs": ["build/**"]
    },
    "web#build": {
      "dependsOn": ["^ui#build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "electronrenderer#build": {
      "dependsOn": ["^ui#build"],
      "outputs": ["build/**"]
    },
    "electronmain#build": {
      "outputs": ["dist/**"]
    },
    "electronbuilder#build": {
      "dependsOn": ["^electronrenderer#build", "^electronmain#build"],
      "outputs": ["dist/**"]
    },
    "build": {},
    "clean": {
      "cache": false
    }
  }
}
